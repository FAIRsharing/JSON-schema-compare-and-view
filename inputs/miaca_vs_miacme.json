          },
          "@id": {
            "format": "uri",
            "description": "The JSON-LD identifier",
            "type": "string"
          },
          "@type": {
            "enum": "Miaca",
            "description": "The JSON-LD type",
            "type": "string"
          },
          "@context": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "object"
